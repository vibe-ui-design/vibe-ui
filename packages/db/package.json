{
  "dependencies": {
    "@acme/id": "workspace:*",
    "@clerk/nextjs": "6.12.8",
    "@supabase/ssr": "0.6.1",
    "@supabase/supabase-js": "2.49.1",
    "@t3-oss/env-core": "0.12.0",
    "@vercel/postgres": "0.10.0",
    "drizzle-orm": "0.41.0",
    "drizzle-seed": "0.3.1",
    "drizzle-zod": "0.7.0",
    "lodash-es": "4.17.21",
    "next": "15.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "shadcn": "2.4.0-canary.17",
    "zod": "3.24.2",
    "zustand": "5.0.3"
  },
  "devDependencies": {
    "@acme/tsconfig": "workspace:*",
    "@t3-oss/env-core": "0.12.0",
    "@t3-oss/env-nextjs": "0.12.0",
    "@types/lodash-es": "4.17.12",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "dotenv-cli": "8.0.0",
    "drizzle-kit": "0.30.5",
    "supabase": "2.19.7",
    "tsx": "4.19.3",
    "typescript": "5.8.2"
  },
  "exports": {
    ".": {
      "default": "./src/index.ts",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "default": "./src/client.ts",
      "types": "./dist/client.d.ts"
    },
    "./schema": {
      "default": "./src/schema.ts",
      "types": "./dist/schema.d.ts"
    },
    "./supabase/client": {
      "default": "./src/supabase/client-exports.ts",
      "types": "./dist/supabase/client-exports.d.ts"
    },
    "./supabase/server": {
      "default": "./src/supabase/server-exports.ts",
      "types": "./dist/supabase/server-exports.d.ts"
    }
  },
  "license": "MIT",
  "name": "@acme/db",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf .turbo node_modules dist",
    "dev": "tsc --watch",
    "gen-migration": "pnpm with-env drizzle-kit generate",
    "migrate": "pnpm with-env drizzle-kit migrate",
    "push": "pnpm with-env drizzle-kit push",
    "seed": "pnpm with-env tsx src/seed.ts",
    "studio": "pnpm with-env drizzle-kit studio",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "type": "module",
  "version": "0.1.0"
}
